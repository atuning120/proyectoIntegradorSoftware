type Carrito {
  id: ID!
  idUsuario: ID!
  idProductos: [ID!]!
}

input AgregarProductoInput {
  IDUsuario: ID!
  IDProducto: ID!
}

input EliminarProductoInput {
  idCarrito: ID!
  idProducto: ID!
}

type Mutation {
  AgregarProducto(input: AgregarProductoInput!): Carrito!
  EliminarProducto(input: EliminarProductoInput!): Carrito!
}